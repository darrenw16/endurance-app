{
  "name": "endurance-app",
  "version": "1.0.0",
  "description": "24-Hour Endurance Racing Pit Strategy App - Real-time race management with pit stop strategy, driver rotation, and FCY handling",
  "keywords": ["racing", "endurance", "pit-strategy", "motorsport", "react", "typescript", "material-design"],
  "homepage": "https://github.com/darrenwalsh/endurance-app#readme",
  "bugs": {
    "url": "https://github.com/darrenwalsh/endurance-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darrenwalsh/endurance-app.git"
  },
  "license": "MIT",
  "author": {
    "name": "Darren Walsh",
    "email": "your-email@example.com"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:verbose": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --no-coverage",
    "test:debug": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --no-coverage --detectOpenHandles --forceExit",
    "test:single": "NODE_OPTIONS=--experimental-vm-modules jest --testNamePattern",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
